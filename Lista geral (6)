#include<stdio.h>

/*
Fa√ßa um programa que leia e armazene dois vetores a e b com 5 elementos cada e apresente o
resultado de:
*/

int main(){

    int vecA[5]={1,2,3,4,5};
    int vecB[5]={6,7,8,9,10};

    int resultado=0;

    for(int i=0;i<4;i++){

        resultado += (vecA[i]+vecB[4-i]);

    }

    printf("%d",resultado);

}
