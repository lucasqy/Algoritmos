/*
    Desenvolva um programa que imprima 10 elementos. Realize esse algoritmo através de
    uma função.
*/

#include<stdio.h>
#include<stdlib.h>
#include<conio.h>
#include<math.h>
#include<locale.h>
#include<string.h>

int function(int a, int b){

    int h;

    h= sqrt(a*a) + (b*b);

    return h;
}

int main(){

    int h;
    int a, b;

    setlocale(LC_ALL, "Portuguese"); 
    
    printf("Entre com os valores de a e b:\n");
    scanf("%d",&a);
    scanf("%d",&b);

    printf("O valor da hipotenusa é:%d",function(a,b));
}
