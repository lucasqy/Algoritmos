#include<stdio.h>
#include<string.h>

int main(){

    char string1[50]="O rato roeu a roupa do rei de roma";
    char string2[50]="O rato roeu a roupa do rei de roma";

    int resultado=0;

    resultado = strcmp(string1,string2);

    if(resultado == 0){
        printf("As duas strings sao iguais.");
    }

    else{
        printf("As duas strings sao diferentes.");
    }

}
